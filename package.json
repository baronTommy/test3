{
  "name": "@tommy_baron/git-test-",
  "description": "Conventional Commits tool",
  "version": "0.7.2",
  "author": "baronTommy",
  "main": "lib/index.js",
  "bin": {
    "interactive-commit-message": "./bin/run"
  },
  "dependencies": {
    "@types/chalk": "2.2.0",
    "chalk": "4.1.2",
    "cosmiconfig": "7.0.1",
    "inquirer": "8.2.0",
    "inquirer-search-list": "1.2.6",
    "table": "6.7.2",
    "tsc-alias": "1.3.10"
  },
  "devDependencies": {
    "@tommy_baron/git-test-": "0.7.1",
    "@types/inquirer": "8.1.3",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.0",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.2.1",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "jscpd": "3.3.26",
    "lint-staged": "11.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "files": [
    "/bin",
    "/lib",
    "README.md"
  ],
  "homepage": "https://github.com/baronTommy/interactive-commit-message",
  "keywords": [
    "commit",
    "conventionalcommits",
    "conventional changelog",
    "semantic-version"
  ],
  "license": "MIT",
  "repository": "wip/interactive-commit-message",
  "scripts": {
    "quality": "run-p quality:*",
    "quality:lint-fmt": "run-s lint fmt",
    "quality:unitTest": "jest",
    "quality:jscpd": "jscpd ./src",
    "quality:type": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "fmt": "prettier --write --ignore-path .gitignore .",
    "preCommit": "yarn quality",
    "prepack": "rm -rf lib && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "prepare": "husky install"
  }
}
